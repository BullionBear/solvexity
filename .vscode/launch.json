{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--reload"
            ],
            "jinja": true
        },
        {
            "name": "Python Debugger: Run backtest argument parser with history",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/argparser/backtest_argparser.py",
            "args": [
                "-c",
                "config/strategy_test.json5"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            }
        },
        {
            "name": "Python Debugger: Run feed argument parser with realtime",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/argparser/feed_argparser.py",
            "args": [
                "-c",
                "config/system/local_test.json5"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            }
        },
        {
            "name": "Python Debugger: Run historical arguement parser",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/argparser/historical_argparser.py",
            "args": [
                "-c",
                "config/system/local_test.json5"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            }
        },
    ]
}