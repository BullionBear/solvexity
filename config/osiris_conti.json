{
    "stream": "TRADE",
    "consumer": {
        "name": "TRADE_FANOUT_BTCUSDT_CONTI",
        "description": "Pub-sub consumer for trade data broadcasting",
        "durable_name": "TRADE_FANOUT_BTCUSDT_CONTI",
        "deliver_policy": "by_start_time",
        "opt_start_time": "2025-10-10T01:00:00.000000Z", 
        "ack_policy": "none",
