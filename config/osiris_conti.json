        "max_deliver": 1,
        "filter_subject": "trade.binance.spot.btcusdt",
        "replay_policy": "instant",
        "sample_freq": "",
        "rate_limit_bps": 10000000,
        "max_ack_pending": 0,
        "idle_heartbeat": 0,
